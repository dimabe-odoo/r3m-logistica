<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="main_data_change_report_account" forcecreate='True' model="ir.cron">
      <field name="name">Cambios Principales de Reportes RRHH</field>
      <field eval="True" name="active"/>
      <field name="user_id" ref="base.user_root"/>
      <field name="interval_number">1</field>
      <field name="interval_type">hours</field>
      <field name="numbercall">-1</field>
      <field name="model_id" ref="dimabe_rrhh.model_hr_payslip"/>
      <field name="state">code</field>
      <field name="code">model.custom_report_fix([
        {'name': 'Nómina',
         'new_name': 'Nómina',
         'template_old': 'hr_payroll.report_payslip_lang',
         'template_new': 'dimabe_rrhh.report_payslip',
         'id': env.ref('hr_payroll.action_report_payslip').id,
         'paperformat_id': env.ref('dimabe_rrhh.paper_format_letter_custom').id,
         'print_report_name': '(object._get_custom_report_name())'
         },

      ])</field>
    </record>

  </data>
</odoo>